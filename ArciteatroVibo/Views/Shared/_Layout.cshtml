<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - ArciteatroVibo</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/ArciteatroVibo.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/lib/font-awesome/css/all.min.css" />
    <link rel="stylesheet" href="~/lib/fontsource/montserrat/400.css" />
</head>
<body>
    <header>
        <div class="TopNavbar">
            <h3 class="text-white">Arci teatro Popolare di Vibo Valentia</h3>
            
        </div>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-0 py-0 border-bottom border-danger" id="navbarCustom">
            <div class="container-fluid">
              
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between" >
                    <ul class="navbar-nav flex-grow-1 justify-content-center" >
                        <li class="nav-item">
                            <a class="nav-link " style="color:#9b1b1b" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link " style="color:#9b1b1b" asp-area="" asp-controller="Eventi" asp-action="Create">Eventi</a>
                        </li>
                        <li class="nav-item  bg-black p-0">
                            <a class="nav-link text-white" asp-area="" asp-controller="Home1" asp-action="Index">ArciteatroVibo</a>
                        </li>
                       
                        <li class="nav-item">
                            <a class="nav-link" style="color:#9b1b1b" asp-area="" asp-controller="Laboratorios" asp-action="Index">Laboratorio</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" style="color:#9b1b1b" asp-area="" asp-controller="Home" asp-action="Privacy">Contatti</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2024 - ArciteatroVibo - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/jsCustom/JavaScript.js" asp-append-version="true"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
