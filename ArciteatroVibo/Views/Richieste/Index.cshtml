@model IEnumerable<ArciteatroVibo.Models.Richieste>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nome)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cognome)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CorpoRichiesta)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FkLaboratorioNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FkUtenteNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cognome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CorpoRichiesta)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FkLaboratorioNavigation.IdLaboratorio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FkUtenteNavigation.IdUtente)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdRichiesta">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdRichiesta">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdRichiesta">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
